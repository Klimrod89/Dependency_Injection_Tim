@page "/"
@inject Demo demo
@inject ProcessDemo processDemo

<PageTitle>Index</PageTitle>

<h1>Hello, world!</h1>
<h2>Demo Time: @demo.StartupTime.ToString("hh:mm:ss fffffff")</h2>
