@page "/"
@* @inject IDemo demo1
@inject IDemo demo2 *@
@inject ProcessDemo processDemo
@inject IEnumerable<IDemo> demos

<PageTitle>Index</PageTitle>

<h1>Hello, world!</h1>
@* <h2>Demo Time: @demo1.StartupTime.ToString("hh:mm:ss fffffff")</h2>
<h2>Demo Time: @demo2.StartupTime.ToString("hh:mm:ss fffffff")</h2> *@


@foreach(var item in demos)
{
    <h2>Demo: @item.StartupTime.ToString("hh:mm:ss fffffff")</h2>
    if(item is ILocalDemo) 
    {
        <h2>Local Demo: @item.StartupTime.ToString("hh:mm:ss ffffff")</h2>
    }
    if(item is IUtcDemo) 
    {
        <h2>UTC Demo: @item.StartupTime.ToString("hh:mm:ss ffffff")</h2>
    }
}